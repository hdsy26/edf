<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-3.2.xsd">
	
	<bean id="xsBeanToXmlConverter" class="org.edf.hifox.core.converter.XsBeanToXmlConverter" />
	<bean id="xsXmlToBeanConverter" class="org.edf.hifox.core.converter.XsXmlToBeanConverter" />
	<bean id="xsRuleCreator" class="org.edf.hifox.core.converter.creator.XsRuleCreator" />
	
	<bean id="xsBeanToJsonConverter" class="org.edf.hifox.core.converter.XsBeanToJsonConverter" />
	<bean id="xsJsonToBeanConverter" class="org.edf.hifox.core.converter.XsJsonToBeanConverter" />
	<bean id="xsJsonRuleCreator" class="org.edf.hifox.core.converter.creator.XsJsonRuleCreator" />
	
	<bean id="jaxbBeanToXmlConverter" class="org.edf.hifox.core.converter.JaxbBeanToXmlConverter" />
	<bean id="jaxbXmlToBeanConverter" class="org.edf.hifox.core.converter.JaxbXmlToBeanConverter" />
	<bean id="jaxbRuleCreator" class="org.edf.hifox.core.converter.creator.JaxbRuleCreator" />
	
	<bean id="fmBeanToStrConverter" class="org.edf.hifox.core.converter.FmBeanToStrConverter" />
	<bean id="fmRuleCreator" class="org.edf.hifox.core.converter.creator.FmRuleCreator" />
	
</beans>
